{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "default": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/default",
      "sourceRoot": "projects/default/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/default",
            "index": "projects/default/src/index.html",
            "browser": "projects/default/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/default/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/default/src/favicon.ico",
              "projects/default/src/assets"
            ],
            "styles": [
              "projects/default/src/assets/fonts/ibm-plex/css/ibm-plex.css",
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "projects/default/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/default/src/environments/environment.ts",
                  "with": "projects/default/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "default:build:production"
            },
            "development": {
              "buildTarget": "default:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "default:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/default/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/default/src/favicon.ico",
              "projects/default/src/assets"
            ],
            "styles": ["projects/default/src/styles.scss"],
            "scripts": []
          }
        },
        "deploy": {
          "builder": "@angular/fire:deploy",
          "options": {
            "version": 2
          },
          "configurations": {
            "production": {
              "buildTarget": "default:build:production",
              "serveTarget": "default:serve:production"
            },
            "development": {
              "buildTarget": "default:build:development",
              "serveTarget": "default:serve:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "library": {
      "projectType": "library",
      "root": "projects/library",
      "sourceRoot": "projects/library/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/library/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/library/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/library/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/library/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "2e1535ba-58ad-4571-8022-28650acb6001"
  }
}
