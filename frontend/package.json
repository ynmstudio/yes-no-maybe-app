    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@angular/localize": "^13.1.3",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
