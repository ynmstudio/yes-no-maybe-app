{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "config": "ts-node set-env.ts",
    "start": "npm run config && npm run gql:update-schema && run-p run:tool gql:codegen:watch",
    "run:tool": "ng serve --project application-tool",
    "build": "npm run config && ng build --project application-tool --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "gql:update-schema": "graphql get-schema --output='schema.hasura.graphql' --endpoint='http://localhost:8080/v1/graphql' --header 'X-Hasura-Admin-Secret=myadminsecretkey'",
    "gql:update-schema:prod": "graphql get-schema -e prod -p hasura",
    "gql:init": "graphql-codegen init",
    "gql:codegen": "graphql-codegen --config codegen.yml",
    "gql:codegen:watch": "graphql-codegen --watch",
    "gql:codegen:watch:test": "graphql-codegen --config codegen.yml",
    "i18n:init": "ngx-translate-extract --input ./projects/application-tool/src --output ./projects/application-tool/src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:extract": "ngx-translate-extract --input ./projects/application-tool/src --output ./projects/application-tool/src/assets/i18n/{en,de}.json --null-as-default-value --clean --format json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/cdk": "^11.2.4",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@apollo/client": "^3.3.11",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngneat/tailwind": "^5.2.3",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@videogular/ngx-videogular": "^3.0.1",
    "apollo-angular": "^2.4.0",
    "apollo-link-context": "^1.0.20",
    "apollo-link-error": "^1.1.13",
    "apollo-link-queue": "^3.0.0",
    "apollo-link-retry": "^2.2.16",
    "apollo-link-ws": "^1.0.20",
    "apollo3-cache-persist": "^0.9.1",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "fast-fuzzy": "^1.10.8",
    "firebase": "^7.0 || ^8.0",
    "graphql": "^15.5.0",
    "ng-connection-service": "^1.0.4",
    "ng2-pdf-viewer": "^6.3.2",
    "ngx-chips": "^2.2.2",
    "ngx-filesize": "^2.0.16",
    "pdfjs-dist": "~2.5.207",
    "rxjs": "~6.6.6",
    "subscriptions-transport-ws": "^0.9.18",
    "tailwindcss-filters": "^3.0.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "11.0.0",
    "@angular-devkit/architect": "0.1102.4",
    "@angular-devkit/build-angular": "~0.1102.4",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-apollo-angular": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.2.1",
    "@tailwindcss/typography": "0.4.0",
    "@types/core-js": "^2.5.4",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "codelyzer": "^6.0.0",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.3",
    "postcss": "8.2.4",
    "protractor": "~7.0.0",
    "sinon": "^9.2.4",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "2.0.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
